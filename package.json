{
  "name": "duet",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "nodemon": "nodemon ./bin/www",
    "peer": "node ./server.js",
    "dev": "concurrently \"npm start\" \"npm run peer\"",
    "dev:yarn": "concurrently \"yarn run nodemon\" \"yarn run peer\""
  },
  "dependencies": {
    "bulma": "^0.9.1",
    "concurrently": "^5.3.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "nanoid": "^3.1.16",
    "node-sass-middleware": "0.11.0",
    "peer": "^0.5.3",
    "pug": "2.0.0-beta11",
    "socket.io": "^2.3.0"
  }
}
